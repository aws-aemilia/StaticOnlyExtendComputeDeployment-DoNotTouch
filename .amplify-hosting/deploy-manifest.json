{
  "version": 1,
  "framework": { "name": "react", "version": "18.2.0" },
  "routes": [
    {
      "path": "/assets/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=31536000, immutable"
      }
    },
    {
      "path": "/screenshots/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=31536000, immutable"
      }
    },
    {
      "path": "/todo*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=0"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=0"
      }
    }
  ]
}